<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>   
	<extension
         point="org.eclipse.ui.commands">
      <category
            id="category"
            name="Key Model">
      </category>
      <command
            categoryId="category"
            name="execute"
            id="execute">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="gui.ButtonHandler"
            commandId="execute">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               id="transformMenu"
               label="PolicyExtractor"
               mnemonic="M">
            <command
                  commandId="execute"
                  id="execute"
                  mnemonic="S"
                  tooltip="execute">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="preferences.PreferenceHandler"
            id="t2"
            name="PolicyExtractor">
      </page>
   </extension>
</plugin>
